create table Sale
{
	ID : Integer { default GetNextGenerator("Sale.ID") }
		tags
		{
		 Frontend.Add.Visible = "false",
		 Frontend.Width = "8"
		},
	Account_ID : Integer tags { Frontend.Width = "8" },
	PurchasedOn : Date { default Date() },
	reference Sale_Account { Account_ID } references Account { ID },
	key { ID }
};
