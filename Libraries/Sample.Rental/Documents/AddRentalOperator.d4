create operator AddRental(ARow : typeof(RentedVideo[]))
begin
 update Video 
  set 
  {
   TimesRented := TimesRented + 1
  }
  where ID = ARow.Video_ID;
end;

attach operator AddRental to RentedVideo on { after insert };
