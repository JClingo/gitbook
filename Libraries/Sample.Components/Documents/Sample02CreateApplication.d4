insert
	table
	{
		row { System.Name("Sample02") ID, "Sample02" Description, ".Frontend.Form('Sample.Components', 'Sample02')" StartDocument }
	}
	into Applications;
insert
	table
	{
		row { System.Name("Sample02") Application_ID, System.Name("Sample.Components") Library_Name }
	}
	into ApplicationLibraries;
insert
	table
	{
		row { System.Name("Sample02") Application_ID, "" ThemeDocument, "" DefaultIconImage, 800 DocumentCacheSize, 60 ImageCacheSize, "LoadBinary('Sample.Components', 'Sample02Help')" HelpDocument }
	}
	into WindowsClientApplications;
