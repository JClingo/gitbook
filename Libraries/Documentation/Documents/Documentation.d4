insert table
{
	row { "Summary" Name, "Introductory paragraph(s)" Description },
	row { "Remarks", "Description of object" },
	row { "Example", "Example of use of object" },
	row { "See Also", "Links to other help or similar objects" }
} into TextClass;

insert table
{
 row { "Aggregate" Name, "Aggregate operator" Description },
 row { "Arithmetic", "Arithmetic object"},
 row { "Bitwise", "Bitwise operator"},
 row { "Catalog", "Catalog object"},
 row { "Comparison", "Comparison operator"},
 row { "Conversion", "Conversion object"},
