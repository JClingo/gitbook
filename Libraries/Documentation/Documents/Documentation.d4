 row { "Date-Time", "Date, Time, DateTime, TimeSpan object"},
 row { "Dynamic-Execution", "Dynamic-Execution operator"},
 row { "Extractor", "Extractor object"},
 row { "Frontend", "Frontend object"},
 row { "Generator", "Generator object"},
 row { "GUID", "GUID object"},
 row { "Library", "Library object"},
 row { "List", "List object"},
 row { "Logical", "Logical object"},
 row { "Management", "Management object"},
 row { "Math", "Math operator, e.g. Abs, Sin"},
 row { "Membership", "Membership object"},
 row { "Missing-Information", "Missing-Information object, e.g. nil, IfNil, IsSpecial"},
